@model AlertMessage

    <div class="alert alert-@Model.AlertType alert-dismissible m-2 text-center"role="alert">
        <h4 class="alert-heading"><i class="@Model.icon @Model.icon2 fa-xl"></i>@Model.Title</h4>
        <hr>
        <p class="mb-0">
            @Model.Message
        </p>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    
